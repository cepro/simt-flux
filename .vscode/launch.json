    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "mode": "debug",
            "type": "go",
            "request": "launch",
            "program": "${workspaceFolder}",
            "args": [
                "-f", "deployment/hazelmead_config.yaml"
              ],
            "env": {
                "SUPABASE_ANON_KEY": "${env:SUPABASE_ANON_KEY}",
                "SUPABASE_USER_KEY": "${env:SUPABASE_USER_KEY}"
            }
        },
        {
            "name": "Test Current File",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${relativeFileDirname}",
            "env": {},
            "args": [],
            "showLog": true
        }    
    ]
}